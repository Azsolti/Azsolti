num_str = ""


for a in range(1,100000):
    num_str += str(a)
    sum_list = [n for n in num_str]
    
    result = 0
    for n in sum_list:
    
            result += int(n) ** len(num_str)
        
    
    if int(result) == int(num_str):
        print(f"Found {result}!")
    
    num_str = ""
    result = 0
